<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="750.0" prefWidth="1500" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ForgetPassword.ForgetPasswordMenuViewController">
   <VBox alignment="CENTER" layoutX="600.0" layoutY="125.0" prefHeight="500.0" prefWidth="300.0" spacing="20.0">
      <children>
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="315.0" text="Please choose a question to answer" />
         <TextField fx:id="usernameField" alignment="CENTER" promptText="Username" />
          <ChoiceBox fx:id="securityQuestionChoiceBox" prefWidth="400.0" />
          <TextField fx:id="securityAnswerField" alignment="CENTER" promptText="Enter your answer" />
         <Button fx:id="submitAnswer" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#submitAnswer" prefHeight="25.0" prefWidth="61.0" text="Submit" />
      </children>
   </VBox>
   <Button fx:id="Back" layoutX="14.0" layoutY="706.0" mnemonicParsing="false" onMouseClicked="#goBackToLogin" prefHeight="30.0" prefWidth="90.0" text="Back" />

</AnchorPane>
